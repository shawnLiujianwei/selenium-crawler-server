{
  "mongo": {
    "db": "web_crawler",
    "host": "127.0.0.1",
    "port": 27017
  },
  "nodeLog": {
    "configuration": {
      "appenders": [
        {
          "type": "console"
        }
      ],
      "levels": {
        "[all]": "debug"
      },
      "replaceConsole": true
    }
  },
  "listener": {
    "webdriverApp": {
      "port": 7001
    },
    "driverInstanceApp": {
      "seleniumHub": 4444,
      "phantomCluster": [
        7010,
        7011
      ],
      "seleniumServer": [
        7020
      ]
    }
  },
  "app": {
    "name": "selenium-webserver",
    "script": "app-webdriver/startup.js",
    "mode": "fork"
  }
}