{
  "mongo": {
    "db": "web_crawler",
    "host": "data-eu.dotter.me",
    "port": 3282
  },
  "nodeLog": {
    "configuration": {
      "appenders": [
        {
          "type": "console"
        },
        {
          "type": "dateFile",
          "filename": "/home/ubuntu/logs/selenium-crawler-output.log",
          "pattern": "-yyyy-MM-dd",
          "alwaysIncludePattern": true,
          "backups": 10
        },
        {
          "type": "logLevelFilter",
          "level": "WARN",
          "appender": {
            "type": "dateFile",
            "filename": "/home/ubuntu/logs/selenium-crawler-error.log",
            "pattern": "-yyyy-MM-dd",
            "alwaysIncludePattern": true,
            "backups": 10,
            "level": "debug"
          }
        }
      ],
      "levels": {
        "[all]": "info"
      },
      "replaceConsole": true
    }
  },
  "seleniumHub": {
    "port": 4141,
    "phantom": [
      5100,
      5101,
      5102,
      5103
    ],
    "chrome": [
      6100
    ]
  }
}