{
  "mongo": {
    "db": "web_crawler",
    "host": "data-eu.dotter.me",
    "port": 3282
  },
  "nodeLog": {
    "configuration": {
      "appenders": [
        {
          "type": "console"
        },
        {
          "type": "dateFile",
          "filename": "/home/ubuntu/logs/nts-crawler-output.log",
          "pattern": "-yyyy-MM-dd",
          "alwaysIncludePattern": true,
          "backups": 10
        },
        {
          "type": "logLevelFilter",
          "level": "WARN",
          "appender": {
            "type": "dateFile",
            "filename": "/home/ubuntu/logs/nts-crawler-error.log",
            "pattern": "-yyyy-MM-dd",
            "alwaysIncludePattern": true,
            "backups": 10,
            "level": "debug"
          }
        }
      ],
      "levels": {
        "[all]": "info"
      },
      "replaceConsole": true
    }
  },
  "listener": {
    "webdriverApp": {
      "port": 7001
    },
    "driverInstanceApp": {
      "port": 7002,
      "seleniumHub": 4444,
      "phantomCluster": [
        7010,
        7011,
        7012,
        7013,
        7014,
        7015,
        7016,
        7017,
        7018,
        7019
      ],
      "seleniumServer": [
        7100
      ]
    }
  },
  "app": {
    "name": "selenium-webserver",
    "script": "app-webdriver/startup.js",
    "mode": "cluster",
    "instances": 2
  }
}